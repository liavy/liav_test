					<column-name>ISDEFAULT</column-name>
				</column>
			</field-map>
			<finder-descriptor>
				<method-name>findAll</method-name>
				<method-params/>
				<load-selected-objects 
					lock="read"/>
			</finder-descriptor>
			<finder-descriptor>
				<method-name>findByArchiveStore</method-name>
				<method-params>
					<method-param>java.lang.String</method-param>
				</method-params>
				<load-selected-objects 
					lock="read"/>
			</finder-descriptor>
			<finder-descriptor>
				<method-name>findByPrimaryKey</method-name>
				<method-params>
					<method-param>java.lang.Long</method-param>
				</method-params>
				<load-selected-objects 
					lock="read"/>
			</finder-descriptor>						
		</entity-bean>
		<entity-bean>
			<ejb-name>PackStatusDBBean</ejb-name>
			<table-name>BC_XMLA_POLLING</table-name>
			<field-map 
				key-type="NoKey">
				<field-name>starttime</field-name>
				<column>
					<column-name>STARTTIME</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="PrimaryKey">
