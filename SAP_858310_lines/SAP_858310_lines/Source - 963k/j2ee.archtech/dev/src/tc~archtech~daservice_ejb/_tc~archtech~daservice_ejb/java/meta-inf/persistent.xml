			<ejb-name>ArchStoreConfigBean</ejb-name>
			<table-name>BC_XMLA_CONFIG</table-name>
			<field-map 
				key-type="PrimaryKey">
				<field-name>primKey</field-name>
				<column>
					<column-name>STOREID</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>archivestore</field-name>
				<column>
					<column-name>ARCHIVESTORE</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>storagesystem</field-name>
				<column>
					<column-name>STORAGESYSTEM</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>storetype</field-name>
