					<column-name>WINROOT</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>unixroot</field-name>
				<column>
					<column-name>UNIXROOT</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>destination</field-name>
				<column>
					<column-name>DESTINATION</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>proxyhost</field-name>
				<column>
					<column-name>PROXYHOST</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>proxyport</field-name>
				<column>
					<column-name>PROXYPORT</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>ilmconform</field-name>
				<column>
					<column-name>ILMCONFORM</column-name>
				</column>
			</field-map>
			<field-map 
				key-type="NoKey">
				<field-name>isdefault</field-name>
