<?xml version="1.0"?>
<veri>
<veritables>
<Dbtable name="VERI">
	<properties>
		<description description = "origin VARCHAR 10 not-null default=123"/>
	</properties>
	<columns>
		<column name = "F1">
			<position>1</position>
			<java-sql-type>VARCHAR</java-sql-type>
			<length>10</length>
